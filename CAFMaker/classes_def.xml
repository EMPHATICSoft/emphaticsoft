<!-- Definitions required for saving CAF objects into the ART stream. Can't go
     in StandardRecord because the Wrapper include puts ART dependencies into the
     StandardRecord dict, which we don't want. -->

<lcgdict>
  
  <class name="std::vector<caf::StandardRecord>" />
  <class name="art::PtrVector<caf::StandardRecord>" />

  <class name="art::Wrapper<caf::StandardRecord >" />
  <class name="art::Wrapper<art::PtrVector<caf::StandardRecord> >" />

</lcgdict>
